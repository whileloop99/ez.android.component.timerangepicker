<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape
            android:shape="oval">

            <gradient
                android:type="radial"
                android:startColor="#00217D"
                android:endColor="#00FFFFFF"
                android:gradientRadius="35dp"/>
        </shape>
    </item>
    <item android:left="4dp"
        android:right="4dp"
        android:top="4dp"
        android:bottom="4dp">
        <shape
            android:shape="oval">

            <solid
                android:color="#fff"/>


        </shape>
    </item>
    <item  android:left="6dp"
        android:right="6dp"
        android:top="6dp"
        android:bottom="6dp">
        <shape
            android:shape="oval">

            <solid
                android:color="#000000"/>

            <size
                android:width="120dp"
                android:height="120dp"/>

        </shape>
    </item>


</layer-list>